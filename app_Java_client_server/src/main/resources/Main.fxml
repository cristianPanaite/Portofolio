<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainFxController">
   <children>
      <TabPane layoutY="29.0" prefHeight="371.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="tabClick" onSelectionChanged="#updateTable" text="Participanti inscrisi">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="tableCurrentSituation" layoutX="6.0" layoutY="1.0" prefHeight="340.0" prefWidth="593.0">
                          <columns>
                            <TableColumn fx:id="distantaColumn" prefWidth="75.0" text="Distanta" />
                            <TableColumn fx:id="stilColumn" prefWidth="75.0" text="Stil" />
                            <TableColumn fx:id="numarPersoaneInscriseColumn" prefWidth="172.0" text="Numar persoane inscrise" />
                          </columns>
                        </TableView>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Cautare">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <GridPane prefHeight="372.0" prefWidth="600.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints maxHeight="118.0" minHeight="0.0" prefHeight="0.0" vgrow="SOMETIMES" />
                            <RowConstraints maxHeight="295.0" minHeight="10.0" prefHeight="295.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="301.0" text="Probe" />
                              <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="298.0" text="Persoane" GridPane.columnIndex="1" />
                              <TableView fx:id="tableProbe" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                                <columns>
                                  <TableColumn fx:id="idColumnProbe" prefWidth="38.0" text="Id" />
                                  <TableColumn fx:id="stilColumnProbe" prefWidth="114.0" text="Stil" />
                                  <TableColumn fx:id="distantaColumnProbe" prefWidth="147.0" text="Distanta" />
                                </columns>
                              </TableView>
                              <TableView fx:id="persoaneTable" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                <columns>
                                  <TableColumn fx:id="nameColumn" prefWidth="116.0" text="Nume" />
                                  <TableColumn fx:id="varstaColumn" minWidth="0.0" prefWidth="124.0" text="Varsta" />
                                  <TableColumn fx:id="probIdColumn" prefWidth="59.0" text="ProbaId" />
                                </columns>
                              </TableView>
                           </children>
                        </GridPane>
                     </children></AnchorPane>
            </content>
          </Tab>
            <Tab text="Inscriere">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <GridPane layoutX="6.0" layoutY="6.0" prefHeight="365.0" prefWidth="593.0">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints maxHeight="117.0" minHeight="0.0" prefHeight="27.0" vgrow="SOMETIMES" />
                            <RowConstraints maxHeight="244.0" minHeight="0.0" prefHeight="31.0" vgrow="SOMETIMES" />
                            <RowConstraints maxHeight="344.0" minHeight="10.0" prefHeight="307.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <Label alignment="CENTER" prefHeight="17.0" prefWidth="334.0" text="Nume" />
                              <TextField fx:id="numeField" prefHeight="12.0" prefWidth="296.0" GridPane.columnIndex="1" />
                              <Label alignment="CENTER" prefHeight="17.0" prefWidth="318.0" text="Varsta" GridPane.rowIndex="1" />
                              <TextField fx:id="varstaField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                               <TableView fx:id="tableProbeInscriere" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
                                   <columns>
                                       <TableColumn fx:id="idColumnProbeInscriere" prefWidth="38.0" text="Id" />
                                       <TableColumn fx:id="stilColumnProbeInscriere" prefWidth="114.0" text="Stil" />
                                       <TableColumn fx:id="distantaColumnProbeInscriere" prefWidth="147.0" text="Distanta" />
                                   </columns>
                               </TableView>
                              <Button fx:id="addInscriereButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#handleInscriere" prefHeight="25.0" prefWidth="301.0" text="Adauga" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                           </children>
                        </GridPane>
                     </children></AnchorPane>
                </content>
            </Tab>
        </tabs>
      </TabPane>
      <Button fx:id="logOutButton" alignment="CENTER" contentDisplay="RIGHT" layoutX="526.0" mnemonicParsing="false" onMouseClicked="#logOutHandle" prefHeight="25.0" prefWidth="65.0" text="LogOut" />
   </children>
</AnchorPane>
